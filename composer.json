{
  "name": "wearesho-team/yii-tokens",
  "type": "proprietary",
  "keywords": [
    "wearesho",
    "yii",
    "registration",
    "tokens"
  ],
  "require": {
    "php": ">=7.0",
    "yiisoft/yii2": "^2.0.14.2",
    "nesbot/carbon": "^1.22"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.3",
    "vlucas/phpdotenv": "^2.4"
  },
  "autoload": {
    "psr-4": {
      "Wearesho\\Yii\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wearesho\\Yii\\Tests\\": "tests/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Alexander Letnikow",
      "email": "reclamme@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "process-timeout": 1800,
    "platform": {
      "php": "7.0"
    }
  }
}
